<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cn.dao.WarnTheme">

    <insert id="add" parameterType="com.cn.esper.event.BaseEvent">
    INSERT INTO WARNEVENT
    (warnTheme,warnRule,warnNum,author,executiver)
     VALUES
    (#{pojo.warnTheme},
    #{pojo.warnRule},
    #{pojo.warnNum},
    #{pojo.author},
    #{pojo.executiver}
    )
    </insert>

</mapper>